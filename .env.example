# Stack
STACK_NAME=proxenos
WEBUI_PORT=3080

# Tailscale
TS_HOSTNAME=proxenos
TS_AUTHKEY=tskey-auth-REPLACE_ME

# Ollama (remote over Tailscale OR local elsewhere)
# Open WebUI reads OLLAMA_API_BASE_URL; examples:
#   http://oracle-llm:11434
#   http://100.x.y.z:11434
OLLAMA_API_BASE_URL=http://oracle-llm:11434

# Optional: other providers (uncomment and set if you use them)
# OPENAI_API_KEY=sk-your-key
# OPENAI_API_BASE_URL=https://api.openai.com/v1
# ANTHROPIC_API_KEY=your-key
# MISTRAL_API_KEY=your-key

# Image Generation Settings
# Enable or disable image generation feature (default: false)
ENABLE_IMAGE_GENERATION=false
# Specify the image generation model to use (default: automatic1111)
IMAGE_GENERATION_MODEL=automatic1111
# Base URL for the Automatic1111 API (if using automatic1111 model)
AUTOMATIC1111_BASE_URL=http://localhost:7860